{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth-services/tsconfig.app.json"
  },
  "projects": {
    "auth-services": {
      "type": "application",
      "root": "apps/auth-services",
      "entryFile": "main",
      "sourceRoot": "apps/auth-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-services/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "convoc-services": {
      "type": "application",
      "root": "apps/convoc-services",
      "entryFile": "main",
      "sourceRoot": "apps/convoc-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/convoc-services/tsconfig.app.json"
      }
    },
    "email-services": {
      "type": "application",
      "root": "apps/email-services",
      "entryFile": "main",
      "sourceRoot": "apps/email-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email-services/tsconfig.app.json"
      }
    },
    "file-services": {
      "type": "application",
      "root": "apps/file-services",
      "entryFile": "main",
      "sourceRoot": "apps/file-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-services/tsconfig.app.json"
      }
    },
    "notification-services": {
      "type": "application",
      "root": "apps/notification-services",
      "entryFile": "main",
      "sourceRoot": "apps/notification-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-services/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/auth-services"
}